{
  "name": "Quickwords",
  "version": "v0.1.1",
  "description": "A productivity app, to get on with your work quicker",
  "main": "out/main/index.js",
  "scripts": {
    "start": "electron .",
    "dev": "node node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "node node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "lint": "node_modules/.bin/eslint --ext .js,.vue src",
    "test": "mocha --recursive",
    "pack": "build --dir",
    "dist": "build"
  },
  "build": {
    "appId": "co.quickwords.quickwords",
    "productName": "Quickwords"
  },
  "repository": "https://github.com/quickwords/quickwords",
  "keywords": [],
  "author": "Dariusz Czajkowski and Grzegorz TÅ‚uszcz",
  "license": "MIT",
  "devDependencies": {
    "@vue/devtools": "^1.1.0",
    "cross-env": "^5.1.6",
    "electron": "^2.0.0",
    "electron-builder": "^20.24.4",
    "eslint": "^4.19.1",
    "eslint-plugin-vue": "^4.5.0",
    "glob-all": "^3.1.0",
    "laravel-mix": "^2.1.11",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.0",
    "purgecss-webpack-plugin": "^0.22.1",
    "tailwindcss": "^0.6.1"
  },
  "dependencies": {
    "auto-launch": "^5.0.5",
    "brace": "^0.11.1",
    "electron-is-dev": "^0.3.0",
    "electron-store": "^1.3.0",
    "fix-path": "^2.1.0",
    "iohook": "^0.2.0",
    "lodash": "^4.17.10",
    "native-keymap": "^1.2.5",
    "node-fetch": "^2.1.2",
    "robotjs": "^0.5.1",
    "vue": "^2.5.16",
    "vue-emoji-picker": "^1.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "webpack-merge": "^4.1.2"
  },
  "iohook": {
    "targets": [
      "node-51",
      "electron-57"
    ],
    "platforms": [
      "darwin",
      "linux",
      "win32"
    ],
    "arches": [
      "x64"
    ]
  }
}
